$(window).on('load', function() {
  if ($(window).width() > 767) {
      $('.ta-preloader').fadeOut('1000', function() {
          $('.ta-main-header, .ta-main-section, .ta-main-footer.desktop-footer').show();
          slickSliderInitializer();
          homeAnimate()
      });
  } else {
      $('.ta-preloader').hide();
      $('.ta-main-header, .ta-main-section, .ta-main-footer.mobile-footer').show();
      slickSliderInitializer();
      $('#phone-1').addClass('ta-banner-bounce');
      setTimeout(function() {
          $('#phone-2').addClass('ta-banner-bounce');
      }, 500);
  }
});
if ($(window).width() > 767) {
  $('.strip').each(function() {
      var $t = $(this),
          rows = $.trim($t.html()).split('<br>');
      $t.html('');
      $.each(rows, function(i, val) {
          $('<span class="row"></span>').appendTo($t);
          var letters = $.trim(val).split('');
          $.each(letters, function(j, v) {
              v = (v == ' ') ? '&nbsp;' : v;
              $('<span>' + $.trim(v) + '</span>').appendTo($('.row:last', $t));
          });
      });
  });
}(function activateTabFromHash() {
  if (location.hash) {
      var tabLink = document.querySelector('a[href="' + location.hash + '"]');
      if (!tabLink) {
          return false;
      }
      tabLink.click();
      if (location.hash) {
          setTimeout(function() {}, 1);
      }
  }
})();
$(document).ready(function() {
  if ($(window).width() > 990) {
      $('.ta-main-header li > a').removeAttr('data-toggle');
      $('.ta-main-header li > a').removeAttr('role');
      $('.ta-main-header li > a').removeAttr('aria-haspopup');
      $('.ta-main-header li > a').removeAttr('aria-expanded');
      $('.ta-main-header li > a').removeClass('dropdown-toggle');
      $('.ta-main-header li').removeClass('dropdown');
  }
  $('#mc-embedded-subscribe').on('click', function() {});
  $('.agencies').on('click', function() {
      $('a[href="' + $(this).attr('href') + '"]').tab('show');
  });
  var idToToggle = window.location.hash.replace("#", "");
  if (idToToggle) {
      $("#" + idToToggle).collapse('show');
  }
  $('li.close').on('click', function() {
      $('ul.services-sub').hide();
  });
  $('.search-fc > .fa').click(function() {
      $('.ta-main-header .search-bar').animate({
          left: '100%'
      }, 1500, function() {
          $('.fa-search-btn.closed').removeClass('opened');
          $('.fa-search-btn.closed').addClass('closed');
          $('.form-group.search-fc').fadeOut();
      });
  });
  $(document).on('click', '.fa-search-btn.opened', function() {
      console.log('dvsavd');
  });
  $(document).on('click', '.fa-search-btn.closed', function(e) {
      e.preventDefault();
      $('.fa-search-btn.closed').removeClass('closed');
      $(this).addClass('opened');
      $('.form-group.search-fc').show();
      $('.ta-main-header .search-bar').animate({
          left: '0%'
      }, 1500);
  });
  $('.navbar-toggle').click(function() {
      if (!$('#bs-example-navbar-collapse-1').hasClass('in')) {
          $('.home-section-1 #phone-1').css('z-index', '0');
      } else {
          $('.home-section-1 #phone-1').css('z-index', '1');
      }
  });

  function closesubheaders() {
      $('.ta-header-l2').hide();
      $('.home-section-1 #phone-1').css('z-index', '1');
  }
  $('.ta-main-header .navbar-nav > li > a').hover(function() {
      if ($(window).width() > 990) {
          closesubheaders();
          if ($(this).hasClass('has-sub-menu')) {
              var submenuId = '#' + $(this).data('submenu');
              $(submenuId).show();
              var HC2Height = $(submenuId + ' .hc-2').height();
              $(submenuId + ' .hc-1').css({
                  'height': HC2Height
              })
              $('.home-section-1 #phone-1').css('z-index', '0');
          }
      }
  });
  $('.ta-header-l2').mouseleave(function() {
      closesubheaders();
  });
  $('.close-sm').click(function() {
      closesubheaders();
  });
});
$(window).scroll(function() {
  $('.ta-header-l2').hide();
});
var acc = document.getElementsByClassName("accordion");
var i;
for (i = 0; i < acc.length; i++) {
  acc[i].addEventListener("click", function() {
      this.classList.toggle("active");
      var panel = this.nextElementSibling;
      if (panel.style.maxHeight) {
          panel.style.maxHeight = null;
      } else {
          panel.style.maxHeight = panel.scrollHeight + "px";
      }
  });
}
