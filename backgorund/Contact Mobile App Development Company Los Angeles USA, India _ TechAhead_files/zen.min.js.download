function homeAnimate() {
  $('#phone-1').animate({
      top: '18rem'
  }, 3000);
  if ($(window).width() > 999) {
      $('#phone-2').animate({
          top: '9rem'
      }, 3000);
      $('#watch-1').animate({
          top: '-18.3rem'
      }, 3000, function() {
          $('#phone-1').addClass('ta-banner-bounce');
          setTimeout(function() {
              $('#phone-2').addClass('ta-banner-bounce');
          }, 500);
          $('.hb-divider').animate({
              width: '15%'
          }, 1500, function() {
              $('.home-section-1 .contact-us').show();
          });
      });
  } else if ($(window).width() <= 999) {
      $('#phone-2').animate({
          top: '-23rem'
      }, 3000);
      $('#watch-1').animate({
          top: '-51rem'
      }, 3000, function() {
          $('#phone-1').addClass('ta-banner-bounce');
          setTimeout(function() {
              $('#phone-2').addClass('ta-banner-bounce');
          }, 500);
          $('.hb-divider').animate({
              width: '15%'
          }, 1500, function() {
              $('.home-section-1 .contact-us').show();
          });
      });
  }
  setTimeout(function() {
      $('.home-section-1 .rounded-rectangle-home').css('overflow', 'visible');
  }, 2300);
}

